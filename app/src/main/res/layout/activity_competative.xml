<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.srajan.edumantra.Competative"
    android:background="@drawable/background">

    <ImageView
        android:layout_width="150dp"
        android:layout_height="130dp"
        android:src="@drawable/mpsc"
        android:layout_marginLeft="25dp"
        android:id="@+id/imgaftertensci"
        android:layout_marginTop="5dp"/>

    <ImageView
        android:layout_marginTop="5dp"
        android:layout_width="150dp"
        android:layout_height="130dp"
        android:src="@drawable/upsc"
        android:layout_toRightOf="@id/imgaftertensci"
        android:layout_marginLeft="30dp"
        />

    <ImageView
        android:layout_width="370dp"
        android:layout_height="300dp"
        android:src="@drawable/fine"
        android:id="@+id/bank"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="150dp"
        android:layout_height="90dp"
        android:layout_alignTop="@+id/imgaftertensci"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="95dp"
        android:src="@drawable/banking"
        android:layout_below="@id/bank"
        android:id="@+id/imageView13" />

</RelativeLayout>
